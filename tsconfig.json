{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": [
        "./src/*",
      ]
    },
    "typeRoots": [
      "node_modules/@types",
    ],
  },
  "include": [
    "next-env.d.ts",
    "src",
    "**/*.ts",
    "**/*.tsx",
    "src/components/jumbotron/jumbotron.tsx",
    "src/components/card/Cards.tsx",
    "src/components/header/Navbar.tsx",
    "src/components/Parceiros/parcerias.tsx",
    "src/components/Sobre/about.tsx",
    "src/pages/missao-valores/index.tsx",
    "src/pages/quem-somos/index.tsx",
  ],
  "exclude": [
    "node_modules"
  ]
}